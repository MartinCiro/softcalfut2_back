model Usuario {
  id        Int    @id @default(autoincrement())
  username  String @unique @map("username")
  nombres   String
  apellidos String      
  pass      String                                 
  id_rol    Int
  id_estado Int

  rol    Rol    @relation(fields: [id_rol], references: [id], onDelete: Cascade, onUpdate: NoAction)
  estado Estado @relation(fields: [id_estado], references: [id])

  productos   Productos[]
  @@map("usuario") 
}
