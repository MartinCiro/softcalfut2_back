model Productos {
  id          Int       @id @default(autoincrement())
  nombre      String    @unique @map("nombre_producto")
  description String
  notas       String
  cantidad    Int
  id_origen   Int
  precio      Decimal
  id_estado   Int
  id_usuario  Int

  usuario     Usuario   @relation(fields: [id_usuario], references: [id])
  estado      Estado    @relation(fields: [id_estado], references: [id])
  origen_producto     Origen  @relation(fields: [id_origen], references: [id])

  @@map("productos") 
}