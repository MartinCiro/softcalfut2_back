model RolXPermiso {
  id_rol    Int
  id_permiso Int

  rol     Rol     @relation(fields: [id_rol], references: [id], onDelete: Restrict, onUpdate: Cascade)
  permiso Permiso @relation(fields: [id_permiso], references: [id], onDelete: Restrict, onUpdate: Cascade)

  @@id([id_rol, id_permiso])
  @@map("rol_x_permiso")      
}
