model Estado {
  id          Int     @id @default(autoincrement())  
  nombre      String  @unique @map("nombre_estado")
  descripcion String?

  usuarios    Usuario[]
  cedulaDeportiva CedulaDeportiva[]
  estado_anuncio Anuncio[]        @relation("estado_anuncio")
  estado_jugador UsuarioXEquipo[] @relation("estado_jugador")

  @@map("estado") 
}
